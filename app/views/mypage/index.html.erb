<h1>Mypage#index</h1>
<p>Find me in app/views/mypage/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Id </th>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td><%= link_to "Show", mypage_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>